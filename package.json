{
	"name": "focus",
	"displayName": "Focus function",
	"description": "Focus a portion of a document in another editor",
	"version": "0.0.1",
	"publisher": "ironyman",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ironyman/vscode-focus"
	},
	"bugs": {
		"url": "https://github.com/ironyman/vscode-focus/issues"
	},
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:focus.onSelection",
		"onLanguage:javascript",
		"onLanguage:typescript",
		"onLanguage:c",
		"onLanguage:c++"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "focus.onSelection",
				"title": "Focus on selection in a new editor"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "focus.onSelection",
					"group": "myGroup@1"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"dependencies": {
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@types/uuid": "^8.3.4",
		"@types/node": "^17.0.21",
		"@types/vscode": "^1.60.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.5.5"
	}
}
